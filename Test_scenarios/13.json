{
	"testName":"Audio scenario change to cre_audioin",
	"Actions":[
		{
			"sub":
			{
				"topic":"creoir/asr/error",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"sub":
			{
				"topic":"creoir/audioIn/changeResult",
				"payload":{"errors":"OK"},
				"timeout": 3000
			}
		},
		{
			"pub":
			{
				"topic":"creoir/audioIn/setScenario",
				"sleep":3000,
				"payload":{"scenarioName":"cre_audioin"}
			}
		},
		{
			"pub":
			{
				"topic":"creoir/asr/resumeWakeword",
				"sleep":300,
				"payload":{}
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/listening",
				"payload":"any",
				"timeout": 3000
			}
		},
		{
			"play":
			{
				"filePath":"audiofiles/hello_home.wav"
			}
		}
	]
}