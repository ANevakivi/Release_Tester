{
	"testName":"Temperature change",
	"Actions":[
		{
			"sub":
			{
				"topic":"creoir/asr/error",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentNotRecognized",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/listening",
				"payload":"any",
				"timeout": 3000
			}
		},
		{
			"play":
			{
				"filePath":"audiofiles/hello_home.wav"
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentRecognized",
				"payload":{},
				"timeout":6000
			}
		},
		{
			"play":
			{
				"filePath":"audiofiles/set_temperature_to_22_degrees.wav"
			}
		},
		{
			"pub":
			{
				"topic":"creoir/asr/setContext",
				"sleep":300,
				"payload":
				{
					"contextName":"YES_NO",
					"timeOut":0,
					"action_after_result":"resume_to_this_context"
				}
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentRecognized",
				"payload":{},
				"timeout":6000
			}
		},
		{
			"play":
			{
				"filePath":"audiofiles/no_i_mean_bedroom.wav"
			}
		}
		
	]
}