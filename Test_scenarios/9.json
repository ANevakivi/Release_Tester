{
	"testName":"Temperature change",
	"Actions":[
		{
			"sub":
			{
				"topic":"creoir/asr/error",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentNotRecognized",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"pub":
			{
				"topic":"creoir/asr/setContext",
				"sleep":300,
				"payload":
				{
					"contextName":"WUW_HOME"
				}
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/listening",
				"payload":"any",
				"timeout": 3000
			}
		},
		{
			"play":
			{
				"filePath":"audiofiles/hello_home.wav"
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentRecognized",
				"payload":{},
				"timeout":6000
			}
		},
		{
			"pub":
			{
				"topic":"creoir/asr/setContext",
				"sleep":300,
				"payload":
				{
					"contextName":"MAIN_OVS"
				}
			}
		},
		{
			"play":
			{
				"filePath":"audiofiles/set_temperature_22_degrees.wav"
			}
		},
		{
			"pub":
			{
				"topic":"creoir/asr/setContext",
				"sleep":300,
				"payload":
				{
					"contextName":"YESNO",
					"timeOut":0
				}
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentRecognized",
				"payload":{},
				"timeout":6000
			}
		},
		{
			"play":
			{
				"filePath":"audiofiles/no_i_mean_bedroom.wav"
			}
		}

	]
}
