{
	"testName":"Set config",
	"Actions":[
		{
			"sub":
			{
				"topic":"creoir/asr/error",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"pub":
			{
				"topic":"creoir/config/set",
				"sleep":300,
				"payload":
				{
					"utteranceThreshold":4699
				}
			}
		},
		{
			"sub":
			{
				"topic":"creoir/config/current",
				"payload":{"utteranceThreshold":4699},
				"failOnReceived":1
			}
		},
		{
			"pub":
			{
				"topic":"creoir/config/get",
				"sleep":300,
				"payload":{}
			}
		}
	]
}