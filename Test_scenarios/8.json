{
	"testName":"Audio scenario switch",
	"mqtt_pub":[
		{
			"topic":"creoir/asr/setContext",
			"payload":{"contextNames":["wuw_enu"], "timeOut":6500, "action_after_result": "resume_to_idle"}
		}

	],
	"mqtt_sub":[
		{
			"topic":"creoir/asr/error",
			"payload":"Any",
			"timeOut": 0
		},
		{
			"topic":"creoir/asr/intentRecognized",
			"payload":{"intent":"TEMP_SET","grammar":"homecommand"},
			"timeOut": 10000
		},
		{
			"topic":"creoir/asr/intentNotRecognized",
			"payload":"Any",
			"timeOut": -6000
		}
		
	],
	"playBackAudio":[
		{
			"filePath": "foo/bar/test1.wav"
		}
	]
}