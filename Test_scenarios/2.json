{
	"testName":"Password test",
	"Actions":[
		{
			"sub":
			{
				"topic":"creoir/asr/error",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentNotRecognized",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/listening",
				"payload":"any",
				"timeout": 3000
			}
		},
		{
			"play":
			{
				"filePath":"audiofiles/hello_home.wav"
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentRecognized",
				"payload":{"utterance":"open main door"},
				"timeout":7000
			}
		},
		{
			"play":
			{
				"filePath":"audiofiles/open_main_door.wav"
			}
		},
		{
			"sleep":
			{
				"duration":3000
			}
		},
		{
			"pub":
			{
				"topic":"creoir/asr/setContext",
				"sleep":300,
				"payload":
				{
					"contextName":"PASSWORD",
					"timeOut":6000,
					"action_after_result":"resume_to_this_context"
				}
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentRecognized",
				"payload":{"utterance":"voice is the key"},
				"timeout":8000
			}
		},
		{
			"play":
			{
				"filePath":"audiofiles/voice_is_the_key.wav"
			}
		}
	]
}