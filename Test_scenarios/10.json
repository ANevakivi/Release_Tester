{
	"testName":"Grm compile test",
	"Actions":[
		{
			"sub":
			{
				"topic":"creoir/asr/error",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/compileResult",
				"payload":{"resultCode":0}
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/compilerReleased",
				"payload":{}
			}
		},
		{
			"pub":
			{
				"topic":"creoir/asr/compileGrammar",
				"sleep":300,
				"payload":
				{
					"grammarName":"COMPILETEST",
					"grammarFilepath":"../usr/share/creoir/bnf/compiletest.bnf"
				}
			}
		},
		{
			"pub":
			{
				"topic":"creoir/asr/compileGrammar",
				"sleep":300,
				"payload":
				{
					"grammarName":"COMPILETEST",
					"grammarFilepath":"../usr/share/creoir/bnf/compiletest.bnf"
				}
			}
		}
	]
}