{
	"testName":"timeout",
	"Actions":[
		{
			"sub":
			{
				"topic":"creoir/asr/error",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentRecognized",
				"payload":"any",
				"failOnReceived":1
			}
		},
		{
			"sub":
			{
				"topic":"creoir/asr/intentNotRecognized",
				"payload":"any",
				"timeout":6000
			}
		},
		{
			"pub":
			{
				"topic":"creoir/asr/setContext",
				"sleep":300,
				"payload":
				{
					"contextName":"PASSWORD",
					"timeOut":5000
				}
			}
		},
		{
			"sleep":
			{
				"duration":6000
			}
		}
	]
}
